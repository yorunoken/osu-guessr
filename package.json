{
    "name": "osu-guessr",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "frontend:dev": "dotenv -e .env.local -- next dev",
        "frontend:build": "next build",
        "frontend:start": "dotenv -e .env.local -- next start",
        "lint": "next lint",
        "server:dev": "cd server && cargo run",
        "server:build": "cd server && cargo build -r",
        "server:start": "cd server && cargo run -r",
        "build": "bun run frontend:build && bun run server:build",
        "dev": "concurrently \"bun run server:start\" \"bun run frontend:dev\"",
        "start": "concurrently \"bun run server:start\" \"bun run frontend:start\"",
        "restart": "git pull && bun run build && pm2 restart 12"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-radio-group": "^1.2.3",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "@radix-ui/react-toast": "^1.2.6",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "concurrently": "^9.1.2",
        "dotenv-cli": "^8.0.0",
        "framer-motion": "^12.5.0",
        "lucide-react": "^0.474.0",
        "next": "^15.2.3",
        "next-auth": "^5.0.0-beta.25",
        "next-intl": "^3.26.5",
        "next-themes": "^0.4.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-intersection-observer": "^9.16.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@types/fuzzyset.js": "^0.0.5",
        "@types/node": "^20.17.27",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "@types/string-similarity": "^4.0.2",
        "eslint": "^9.23.0",
        "eslint-config-next": "15.1.6",
        "postcss": "^8.5.3",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.8.2"
    }
}
